  "source": "pragma solidity 0.4.24;\n\ncontract TestNonce {\n  uint value;\n\n  event NewValueSet(uint _value);\n\n  function set(uint _value) public {\n    // require (value <= 10, \"value <= 10\");\n    value = _value;\n    emit NewValueSet(value);\n  }\n\n  function get() public view returns (uint) {\n    return value;\n  }\n}",
